S3method(mPPCA_Gibbs,default,mPPCA_Gibbs.default)
export("mPPCA_Gibbs")
export("mPPCA_Gibbs.formula")
export("mPPCA_Gibbs.default")
S3method(mBMF_Gibbs,formula,mBMF_Gibbs.formula)
S3method(mBMF_Gibbs,default,mBMF_Gibbs.default)
export("mBMF_Gibbs")
export("mBMF_Gibbs.formula")
export("mBMF_Gibbs.default")
S3method(prod_mPPCA,formula,prod_mPPCA.formula)
S3method(prod_mPPCA,default,prod_mPPCA.default)
export("prod_mPPCA")
export("prod_mPPCA.formula")
export("prod_mPPCA.default")
export("sparse_model_matrix_b")
